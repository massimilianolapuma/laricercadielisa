{
    "name": "laricercadielisa",
    "version": "1.0.0",
    "description": "A Chrome extension for searching through all opened tabs",
    "type": "module",
    "scripts": {
        "build": "node build.js",
        "build:major": "node build.js major",
        "build:minor": "node build.js minor",
        "build:patch": "node build.js patch",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "test:coverage": "vitest --coverage",
        "test:coverage:ci": "vitest run --coverage",
        "test:run": "vitest run",
        "lint": "eslint popup.js tests/",
        "lint:fix": "eslint popup.js tests/ --fix",
        "dev": "npm run test"
    },
    "devDependencies": {
        "@vitest/ui": "^3.2.3",
        "@vitest/coverage-v8": "^3.2.3",
        "eslint": "^8.0.0",
        "jsdom": "^23.0.0",
        "vitest": "^3.2.3"
    },
    "keywords": [
        "chrome-extension",
        "tab-search",
        "browser",
        "productivity"
    ],
    "author": "",
    "license": "MIT"
}